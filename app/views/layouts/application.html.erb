<!DOCTYPE html>
<html>
<head>
  <title>AGENTSITE</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <link href="https://cdnjs.cloudflare.com/ajax/libs/unslider/2.0.3/css/unslider.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/core.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/unslider/2.0.3/js/unslider-min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>



  <%= csrf_meta_tags %>

</head>
<body>

  <nav>
    <div class="nav-wrapper">
      <a href="#" class="brand-logo"><%= image_tag('logo.jpg', :class => "spclogo" ) %></a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href="#">Logout</a></li>
        <li><a href="#">About Us</a></li>
        <li><a href="#">Contact</a></li>
      </ul>
    </div>
  </nav>

    <div class="container">
<%= yield %>
    </div>

      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>



</body>
</html>
